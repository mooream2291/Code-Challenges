'use strict';

let n = [0,1,3,6,7,8,9,10,12,13,7,15,20]
//declare the function that takes in n as an argument
function isPrime(n) {
  let n = [0,1,3,6,7,8,9,10,12,13,7,15,20]
//for loop to iterate over array of numbers
//create a new array to
let nums = [];
for (let i = 0; i < n; i++) {
  nums[i] == true;
}

for (let i = 2; i * i < n; i++) {
  if(!nums[i]) {
    continue;
  };
  for (let j = i * i; j < n; j += i) {
    nums[j] = 0;
  };
  return nums.reduce( (a,b) => b+a) - 2;
}
}